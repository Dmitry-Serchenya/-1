#include <stdio.h>
#include <math.h>
int main(void) { 

double a, b, c, arithmetic, geometric;
printf("Enter a: ");
  scanf("%lf", &a);
  printf("Enter b: ");
    scanf("%lf", &b);
    printf("Enter c: ");
    scanf("%lf", &c); 
arithmetic = (a+b+c)/3;
     geometric = pow((a*b*c), 1/3.0);
  if (a == 0 || b == 0  || c == 0   ) 
     printf("%.2lf\n",arithmetic);
  else 
    printf("%.2lf\n",geometric);   
  return 0;
}
